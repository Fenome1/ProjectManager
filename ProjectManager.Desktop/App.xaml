<Application x:Class="ProjectManager.Desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="/View/Manager/ManagerWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <FontFamily x:Key="WhitneyBlack">pack://application:,,,/Assets/Fonts/Whitney/Whitney-Black.ttf#Whitney Black</FontFamily>
            <FontFamily x:Key="WhitneyMedium">pack://application:,,,/Assets/Fonts/Whitney/Whitney-Medium.ttf#Whitney Medium</FontFamily>

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Styles/Themes/SecondaryThemeDictionary.xaml" />
                <ResourceDictionary Source="View/Styles/Themes/PrimaryThemeDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>